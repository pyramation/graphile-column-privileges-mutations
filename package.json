{
  "name": "graphile-column-privileges-mutations",
  "version": "0.0.7",
  "description": "graphile mutations for tables with column privileges",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/graphile-column-privileges-mutations#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module"
  ],
  "scripts": {
    "build:main": "BABEL_ENV=production babel src --out-dir main --delete-dir-on-start",
    "build:module": "MODULE=true babel src --out-dir module --delete-dir-on-start",
    "build": "npm run build:module && npm run build:main",
    "prepublish": "npm run build",
    "lint": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/graphile-column-privileges-mutations"
  },
  "keywords": [
    "postgraphile",
    "graphile",
    "plugin",
    "postgres",
    "rls",
    "security",
    "graphql",
    "metaschema"
  ],
  "bugs": {
    "url": "https://github.com/pyramation/graphile-column-privileges-mutations/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-plugin-macros": "2.8.0",
    "envalid": "^6.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.11.0",
    "jest": "^24.5.0",
    "mock-req": "0.2.0",
    "pg": "^8.3.3",
    "pg-query-string": "^0.1.4",
    "postgraphile": "^4.9.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "debug": "^4.2.0"
  }
}
